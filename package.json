{
  "name": "create-next-app-experimental-mantine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "npm update": "npm update",
    "dev": "next dev",
    "dev turbo": "next dev --turbo",
    "build && start": "next build && next start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase login": "npx dotenv -e .env.local -- bash -c 'echo \"$SUPABASE_ACCESS_TOKEN\" | npx -S supabase login'",
    "supabase init": "mkdir -p services/ && npx supabase init --workdir services/",
    "supabase projects list": "npx supabase projects list",
    "supabase link --project-ref": "npx dotenv -e .env.local -- bash -c 'npx supabase --workdir services/ link  --project-ref $SUPABASE_PROJECT_ID -p $SUPABASE_DB_PASSWORD'",
    "type gen": "mkdir -p services/supabase/lib && npx supabase --workdir services/ gen types typescript --linked > services/supabase/lib/database.types.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/carousel": "^5.10.4",
    "@mantine/core": "^5.10.4",
    "@mantine/hooks": "^5.10.4",
    "@mantine/next": "^5.10.4",
    "@mantine/spotlight": "^5.10.4",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-helpers-shared": "^0.2.4",
    "@supabase/auth-ui-react": "^0.2.8",
    "@supabase/supabase-js": "^2.8.0",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "classnames": "^2.3.2",
    "embla-carousel-autoplay": "^7.0.9",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "jotai": "^2.0.2",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-bottom-scroll-listener": "^5.1.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "server-only": "^0.0.1",
    "supabase": "^1.38.7",
    "typescript": "4.9.5",
    "typescript-plugin-css-modules": "^4.2.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0"
  }
}
